      "required": false,
      "path": "57.inputs.steps",
      "default": 4
    },
    {
      "name": "cfg",
      "type": "number",
      "description": "分类器引导强度,控制提示词对生成的影响程度",
      "required": false,
      "path": "57.inputs.cfg",
      "default": 1
    },
    {
      "name": "sampler_name",
