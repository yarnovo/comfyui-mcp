{
  "name": "wan2_2_ti2v_5b",
  "description": "使用 WAN 2.2 5B 模型从文本描述生成高质量视频的专业工具。这是一个纯文本到视频的先进模型，无需输入图片即可直接生成动态视频内容。采用 T5-XXL 文本编码器实现精确的语义理解，SD3 采样算法确保稳定的视频生成质量。支持生成最高1280x704分辨率、121帧（约5秒@24fps）的流畅视频。特别适合创意视频制作、概念可视化、动画生成等专业应用场景。",
  "category": "text-to-video",
  "parameters": [
    {
      "name": "positive_prompt",
      "type": "string",
      "description": "描述想要生成的视频内容，包括场景、动作、风格、镜头运动等。越详细效果越好",
      "required": true,
      "default": "",
      "path": "6.inputs.text"
    },
    {
      "name": "negative_prompt",
      "type": "string",
      "description": "负向提示词，描述不想要出现的内容，如模糊、静止、低质量等",
      "required": false,
      "default": null,
      "path": "7.inputs.text"
    },
    {
      "name": "width",
      "type": "number",
      "description": "视频宽度（像素）。建议使用16的倍数以获得最佳效果",
      "required": false,
      "default": 1280,
      "min": 256,
      "max": 1920,
      "path": "55.inputs.width"
    },
    {
      "name": "height",
      "type": "number",
      "description": "视频高度（像素）。建议使用16的倍数以获得最佳效果",
      "required": false,
      "default": 704,
      "min": 256,
      "max": 1080,
      "path": "55.inputs.height"
    },
    {
      "name": "length",
      "type": "number",
      "description": "视频总帧数。121帧约5秒（24fps），可根据需要调整",
      "required": false,
      "default": 121,
      "min": 16,
      "max": 256,
      "path": "55.inputs.length"
    },
    {
      "name": "batch_size",
      "type": "number",
      "description": "批次大小，一次生成多少个视频",
      "required": false,
      "default": 1,
      "min": 1,
      "max": 4,
      "path": "55.inputs.batch_size"
    },
    {
      "name": "fps",
      "type": "number",
      "description": "视频帧率（每秒帧数）",
      "required": false,
      "default": 24,
      "min": 8,
      "max": 60,
      "path": "57.inputs.fps"
    },
    {
      "name": "seed",
      "type": "number",
      "description": "随机种子，用于生成结果的可重复性。使用相同种子可获得相同结果",
      "required": false,
      "default": 898471028164125,
      "min": 0,
      "max": 999999999999999,
      "path": "3.inputs.seed"
    },
    {
      "name": "steps",
      "type": "number",
      "description": "采样步数，影响生成质量。更多步数质量更好但速度更慢",
      "required": false,
      "default": 20,
      "min": 10,
      "max": 50,
      "path": "3.inputs.steps"
    },
    {
      "name": "cfg",
      "type": "number",
      "description": "分类器引导强度，控制提示词对生成的影响程度。较高值更严格遵循提示词",
      "required": false,
      "default": 5,
      "min": 1,
      "max": 20,
      "path": "3.inputs.cfg"
    },
    {
      "name": "sampler_name",
      "type": "string",
      "description": "采样器名称，不同采样器有不同的生成特性",
      "required": false,
      "default": "uni_pc",
      "enum": ["euler", "euler_ancestral", "dpmpp_2m", "dpmpp_3m_sde", "uni_pc", "lcm"],
      "path": "3.inputs.sampler_name"
    },
    {
      "name": "scheduler",
      "type": "string",
      "description": "调度器类型，控制采样过程的噪声调度",
      "required": false,
      "default": "simple",
      "enum": ["normal", "karras", "exponential", "simple", "beta"],
      "path": "3.inputs.scheduler"
    },
    {
      "name": "denoise",
      "type": "number",
      "description": "去噪强度，1.0为完全去噪",
      "required": false,
      "default": 1,
      "min": 0,
      "max": 1,
      "path": "3.inputs.denoise"
    },
    {
      "name": "shift",
      "type": "number",
      "description": "SD3 采样算法的偏移值，影响生成的细节和风格",
      "required": false,
      "default": 8,
      "min": 0,
      "max": 20,
      "path": "48.inputs.shift"
    },
    {
      "name": "filename_prefix",
      "type": "string",
      "description": "输出视频文件名前缀",
      "required": false,
      "default": "video/ComfyUI",
      "path": "58.inputs.filename_prefix"
    },
    {
      "name": "video_format",
      "type": "string",
      "description": "视频格式",
      "required": false,
      "default": "auto",
      "enum": ["auto", "mp4", "webm", "avi"],
      "path": "58.inputs.format"
    },
    {
      "name": "codec",
      "type": "string",
      "description": "视频编码器",
      "required": false,
      "default": "auto",
      "enum": ["auto", "h264", "h265", "vp9"],
      "path": "58.inputs.codec"
    }
  ],
  "models": {
    "unet": "wan2.2_ti2v_5B_fp16.safetensors",
    "clip": "umt5_xxl_fp8_e4m3fn_scaled.safetensors",
    "vae": "wan2.2_vae.safetensors"
  },
  "tags": [
    "text-to-video",
    "wan2.2",
    "video-generation",
    "ai-video",
    "5B-model"
  ],
  "output_node": "58",
  "version": "1.0.0"
}