{
  "description": "使用 FLUX.1 Schnell 完整版工作流，支持双CLIP文本编码器实现更精确的提示词控制。采用极速4步生成算法，在保持高质量的同时大幅提升生成速度。特别适合需要快速迭代、实时预览和批量生成的创意工作流程。支持1024x1024及更高分辨率输出。",
  "category": "text-to-image",
  "parameters": [
    {
      "name": "positive_prompt",
      "type": "string",
      "description": "正向提示词（CLIP L），描述想要生成的主要图像内容",
      "required": true,
      "path": "41.inputs.clip_l",
      "default": ""
    },
    {
      "name": "t5xxl_prompt",
      "type": "string",
      "description": "T5-XXL提示词，提供更详细的场景描述和风格指导",
      "required": false,
      "path": "41.inputs.t5xxl",
      "default": null
    },
    {
      "name": "guidance",
      "type": "number",
      "description": "引导强度，控制提示词对生成的影响程度",
      "required": false,
      "path": "41.inputs.guidance",
      "default": 3.5
    },
    {
      "name": "seed",
      "type": "number",
      "description": "随机种子，用于生成结果的可重复性",
      "required": false,
      "path": "31.inputs.seed",
      "default": 832603535778030
    },
    {
      "name": "steps",
      "type": "number",
      "description": "采样步数（Schnell模型优化为4步）",
      "required": false,
      "path": "31.inputs.steps",
      "default": 4
    },
    {
      "name": "cfg",
      "type": "number",
      "description": "CFG强度（Schnell模型推荐使用1）",
      "required": false,
      "path": "31.inputs.cfg",
      "default": 1
    },
    {
      "name": "width",
      "type": "number",
      "description": "图像宽度（像素）",
      "required": false,
      "path": "27.inputs.width",
      "default": 1024
    },
    {
      "name": "height",
      "type": "number",
      "description": "图像高度（像素）",
      "required": false,
      "path": "27.inputs.height",
      "default": 1024
    },
    {
      "name": "batch_size",
      "type": "number",
      "description": "批次大小，一次生成多少张图像",
      "required": false,
      "path": "27.inputs.batch_size",
      "default": 1
    },
    {
      "name": "sampler_name",
      "type": "string",
      "description": "采样器名称",
      "required": false,
      "path": "31.inputs.sampler_name",
      "default": "euler"
    },
    {
      "name": "scheduler",
      "type": "string",
      "description": "调度器类型",
      "required": false,
      "path": "31.inputs.scheduler",
      "default": "simple"
    },
    {
      "name": "denoise",
      "type": "number",
      "description": "去噪强度",
      "required": false,
      "path": "31.inputs.denoise",
      "default": 1
    },
    {
      "name": "unet_name",
      "type": "string",
      "description": "UNet模型文件名",
      "required": false,
      "path": "38.inputs.unet_name",
      "default": "flux1-schnell.safetensors"
    },
    {
      "name": "vae_name",
      "type": "string",
      "description": "VAE模型文件名",
      "required": false,
      "path": "39.inputs.vae_name",
      "default": "ae.safetensors"
    },
    {
      "name": "clip_name1",
      "type": "string",
      "description": "第一个CLIP模型文件名",
      "required": false,
      "path": "40.inputs.clip_name1",
      "default": "clip_l.safetensors"
    },
    {
      "name": "clip_name2",
      "type": "string",
      "description": "第二个CLIP模型文件名（T5-XXL）",
      "required": false,
      "path": "40.inputs.clip_name2",
      "default": "t5xxl_fp16.safetensors"
    },
    {
      "name": "filename_prefix",
      "type": "string",
      "description": "输出文件名前缀",
      "required": false,
      "path": "9.inputs.filename_prefix",
      "default": "ComfyUI"
    }
  ]
}