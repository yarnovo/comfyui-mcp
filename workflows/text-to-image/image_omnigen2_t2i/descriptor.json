{
  "description": "使用 OmniGen2 最新一代模型生成超高质量图像。采用双CFG引导技术，提供更精确的生成控制。特别擅长生成细节丰富、构图精美的图像，支持复杂场景和角色描绘。适合专业级图像创作需求，默认1024x1024分辨率。",
  "category": "text-to-image",
  "parameters": [
    {
      "name": "positive_prompt",
      "type": "string",
      "description": "正向提示词，描述想要生成的图像内容",
      "required": true,
      "path": "6.inputs.text",
      "default": ""
    },
    {
      "name": "negative_prompt",
      "type": "string",
      "description": "负向提示词，描述不想要出现的内容",
      "required": false,
      "path": "7.inputs.text",
      "default": null
    },
    {
      "name": "seed",
      "type": "number",
      "description": "随机种子，用于生成结果的可重复性",
      "required": false,
      "path": "21.inputs.noise_seed",
      "default": 1082931635154736
    },
    {
      "name": "steps",
      "type": "number",
      "description": "采样步数，影响生成质量",
      "required": false,
      "path": "23.inputs.steps",
      "default": 20
    },
    {
      "name": "cfg_conds",
      "type": "number",
      "description": "条件CFG强度",
      "required": false,
      "path": "27.inputs.cfg_conds",
      "default": 5
    },
    {
      "name": "cfg_cond2_negative",
      "type": "number",
      "description": "负向条件CFG强度",
      "required": false,
      "path": "27.inputs.cfg_cond2_negative",
      "default": 2
    },
    {
      "name": "width",
      "type": "number",
      "description": "图像宽度（像素）",
      "required": false,
      "path": "11.inputs.width",
      "default": 1024
    },
    {
      "name": "height",
      "type": "number",
      "description": "图像高度（像素）",
      "required": false,
      "path": "11.inputs.height",
      "default": 1024
    },
    {
      "name": "batch_size",
      "type": "number",
      "description": "批次大小，一次生成多少张图像",
      "required": false,
      "path": "11.inputs.batch_size",
      "default": 1
    },
    {
      "name": "sampler_name",
      "type": "string",
      "description": "采样器名称",
      "required": false,
      "path": "20.inputs.sampler_name",
      "default": "euler"
    },
    {
      "name": "scheduler",
      "type": "string",
      "description": "调度器类型",
      "required": false,
      "path": "23.inputs.scheduler",
      "default": "simple"
    },
    {
      "name": "denoise",
      "type": "number",
      "description": "去噪强度",
      "required": false,
      "path": "23.inputs.denoise",
      "default": 1
    },
    {
      "name": "filename_prefix",
      "type": "string",
      "description": "输出文件名前缀",
      "required": false,
      "path": "9.inputs.filename_prefix",
      "default": "ComfyUI"
    }
  ]
}